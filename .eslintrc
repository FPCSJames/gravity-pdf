{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "globals": {
    "GFPDF": false,
    "Backbone": false,
    "gfpdf_migration_multisite_ids": false,
    "gf_vars": false,
    "tinyMCE": false,
    "gform": false,
    "ConditionalLogic": false,
    "ToggleConditionalLogic": false,
    "QTags": false,
    "switchEditors": false,
    "getUserSetting": false,
    "wp": false,
    "gfMergeTagsObj": false,
    "form": false,
    "gform_initialize_tooltips": false,
    "_": false
  },
  "rules": {
    "semi": ["error", "never"],
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "no-control-regex": 0
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}